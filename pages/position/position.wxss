.banner{
  width:100%;
}

.list{
  width: 100%;
	height: 235rpx;
	background-color: #ffffff;
  border-bottom: 10rpx solid #fafafa;
}

.list .content{
  width:94%;
  height: 235rpx;
  margin:0 auto;
  /* background:#f00; */
}

.list .content>view{
  width: 100%;
  height: 33.33%;
}

.list .content .top{
  /* background:#00f; */
  line-height: 120rpx; 
}

.list .content .centent{
  /* background:#000; */
	line-height: 85rpx;
  
}

.list .content .bottom{
  /* background:#ff0; */
	line-height: 37rpx;
  
}

.top .left,.centent .left{
  width:70%;
  height: 100%;
  float:left;
  font-family: SourceHanSansCN-Regular;
	font-size: 34rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #000000;

}

.top .right,.centent .right{
  width:30%;
  height: 100%;
  float:right;
  text-align: right;
  font-family: ArialMT;
	font-size: 34rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #6ac4d2;
  
}

.centent .left{
  font-family: SourceHanSansCN-Regular;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #d6d6d6;
}

.centent .right{
  font-family: ArialMT;
	font-size: 34rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #e42387;
}

.bottom .logo{
  width:37rpx;
  height: 37rpx;
  float:left;
}
.bottom .logo image{
width:37rpx;
}
.bottom .cname{
  width:92%;
  height: 100%;
  float:right;
  font-family: SourceHanSansCN-Regular;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #d6d6d6;  
}

.line{
  width:100%;
  height: 1px;
  background:#b4b4b4;
  position:fixed;
  bottom: 0rpx;
}

swiper{
  width:100%;
  height: 273rpx;
}
swiper image{
  width: 100%;
}

.top{
  width:100%;
  position:relative;
}
.top image{
  width:100%;
  height:550rpx!important;
}
.contant{
  width:100%;
}
.section{
  width:80%;
  margin:0 auto;
  margin-top: 25rpx;
  height:80rpx;
  border:1px solid #ccc;
}
.section input{
  font-size: 24rpx;
}
.picker{
  width:90%;
  padding: 0 5%;
  height:80rpx;
  line-height: 80rpx;
}

.submit{
  width:80%;
  margin:0 auto;
  margin-top:40rpx;
  background-color: #FF6700;
  color:#fff;
  font-size: 30rpx;
  height:80rpx;
  line-height:80rpx;
}
.title{
  display:block;
  margin-top:30rpx;
  width:100%;
  text-align: center;
  font-size: 40rpx;
  color:#FF6700;
}
.form-container{
  margin-top: 40rpx;
}
.pic-content{
  position:absolute;
  width:80%;
  top:100rpx;
  left:0;
  right:0;
  margin:0 auto;
}
.pic-content-title text{
  display:block;
  width:100%;
  font-size: 58rpx;
  color:#fff;
}
.pic-content-sec{
  margin-top:50rpx;
}
.pic-content-sec text{
  display:block;
  width:100%;
  font-size: 24rpx;
  color:#fff;
  margin-top:15rpx;
}