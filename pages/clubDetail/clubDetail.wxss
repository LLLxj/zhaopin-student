.top{
  width: 94%;
	height: 170rpx;
	background-color: #fff;
  margin:0 auto;
}

.top>view{
  width:100%;
  height: 50%;
}

.top .top_{
  line-height: 100rpx;
}
.top .bottom_{
  line-height: 50rpx;
}
.top>view .left{
  width:70%;
  height: 100%;
  float:left;
}
.top>view .right{
  width:30%;
  height: 100%;
  float:right;
}

.top .top_ .left{
  font-family: SourceHanSansCN-Medium;
	font-size: 42rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #000000;
}
.top .top_ .right{
  font-family: ArialMT;
	font-size: 34rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #6ac4d2;
  text-align: right;
}


.top .bottom_ .left{
  font-family: SourceHanSansCN-Regular;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
