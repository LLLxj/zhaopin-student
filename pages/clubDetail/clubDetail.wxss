}

/* 头部公司信息开始 */
.club_img_info{
  width:100%;
  height:350rpx;
  position:relative;
}
club_img_bor{
  width:100%;
  height:100%;
}
.club_bac{
  width:100%;
}
.club_logo_and_name{
  width:30%;
  border:1rpx solid blue;
  position:absolute;
  height:200rpx;
  top:30rpx;
  left:0;
  right:0;
  margin:0 auto;
}
.club_logo{
  position:absolute;
  width:150rpx;
  height:150rpx;
  top:100rpx;
  left:0;
  right:0;
  margin:0 auto;
  border-radius:50%;
}
.club_name{
  position:absolute;
  top:280rpx;
  left:0;
  right:0;
  margin:0 auto;
  text-align: center;
  font-size:34rpx;
  color:#ffffff;
}
/* 头部公司信息结束 */

/* 导航切换栏开始 */
.navbar{
  /* flex: none; */
  /* display: flex; */
  background: #fff;
}
.navbar .item{
  position: relative;
  width:10%;
  margin-left:23%;
  font-size: 30rpx;
  line-height: 100rpx;
  color:#B1B1B1;
}
.navbar .item.active{
  color: #6ac4d2;
}
.navbar .item.active:after{
  content: "";
  display: block;
  position: absolute;
  bottom: -30rpx;
  left: 0;
  right: 0;
  height: 4rpx;
  background: #6ac4d2;
}
.navbar{
  border-bottom:1rpx solid #c6c6c6;
}
/* 导航切换结束 */

