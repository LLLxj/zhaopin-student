	color: #d6d6d6;
}
.top .bottom_ .right{
  font-family: ArialMT;
	font-size: 34rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #e42387;
  text-align: right;
}

.hr{
  width:100%;
