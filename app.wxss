/**app.wxss**/
/* 最外层 */
.body{
  width:100%;
  height: 100%;
  /* background: #f00; */
}

.body>.content{
  width:100%;
  height: 100%;
  margin:0 auto;
  
}

 view,text{
   overflow:hidden;
  white-space:nowrap;
  text-overflow : ellipsis;
  word-break:break-all; 
} 

.hr{
  width:100%;
  height: 2rpx;
  background: #fafafa;
  float: none;
  clear:both;
}
.nbsp{
  color: #fff;
  
}
.navigator-hover{
  background: #fff;
}




/* 弹窗 */
.alert{
 width: 240rpx;
	height: 240rpx;
	background-color: rgba(0, 0, 0, 0.7);
	border-radius: 10rpx;
	opacity: 0.7;
  position:absolute;
  top:35%;
  left:255rpx; 
  margin:0 auto;
  text-align: center;
}
.alert image{
  width:89rpx;
  margin-top: 50rpx;
}

.alert text{
  display: block;
  width:100%;
  height: 24rpx;
  margin-top: 30rpx;
  text-align: center;
  font-family: SourceHanSansCN-Regular;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 24rpx;
	letter-spacing: 0rpx;
	color: #ffffff;
 
}

.rightimg{
  width:12rpx;
}


.tabBar{
  width:100%;
  height: 96rpx;
  position:fixed;
  bottom:0rpx;
  border-top: 1rpx solid #b4b4b4;
}
.tabBar>view{
  width:33.3%;
  height: 96rpx;
  text-align: center;
  float: left;
}
.tabBar>view>.null{
  width:100%;
  height: 8rpx;
}
.tabBar>view>.image{
  width:100%;
  height: 48rpx;
}
.tabBar>view>.text{
  width:100%;
  height: 40rpx;
  font-family: MicrosoftYaHei;
	font-size: 20rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #7f8389;
}



.tabBar>view>.image>image{
  width:41rpx;
}

.prompt{
  display: none;
  width: 100%;
  height: 105rpx;
  font-family: SourceHanSansCN-Regular;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 105rpx;
	letter-spacing: 0rpx;
	color: #9b9b9b;
  text-align: center;
}